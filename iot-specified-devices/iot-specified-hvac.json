{
    "@context": [
        "https://www.w3.org/2019/wot/td/v1",
        { "saref": "https://w3id.org/saref#" }
    ],
    "id": "id",
    "title": "Heating and Cooling",
    "@type": "saref:Device",
    "securityDefinitions": {"basic_sc": {
        "scheme": "basic",
        "in": "header"
    }},
    "security": ["basic_sc"],
    "properties": {
        "status": {
            "readOnly": true,
            "@type": "saref:OnOffState",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/state"
            }]
        },
        "available": {
            "readOnly": true,
            "@type": "saref:Property",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/available"
            }]
        },
        "temperature": {
            "readOnly": true,
            "@type": "saref:Temperature",
            "minimum": 10.0,
            "maximum": 50.0,
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/temperature"
            }]
        }
    },
    "actions": {
        "close": {
            "@type": "saref:OffCommand",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/off"
            }]
        },
        "open": {
            "@type": "saref:OnCommand",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/on"
            }]
        },
        "setTemperature" : {
            "@type": "saref:Command",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/setTemperature"
            }]
        }
    }
}