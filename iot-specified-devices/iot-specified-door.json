{
    "@context": [
        "https://www.w3.org/2019/wot/td/v1",
        { "saref": "https://w3id.org/saref#" }
    ],
    "id": "id",
    "title": "Door",
    "@type": "saref:Device",
    "securityDefinitions": {"basic_sc": {
        "scheme": "basic",
        "in": "header"
    }},
    "security": ["basic_sc"],
    "properties": {
        "status": {
            "readOnly": true,
            "@type": "saref:OpenCloseState",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/state"
            }]
        },
        "available": {
            "readOnly": true,
            "@type": "saref:Property",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/available"
            }]
        }
    },
    "actions": {
        "close": {
            "@type": "saref:CloseCommand",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/close"
            }]
        },
        "open": {
            "@type": "saref:OpenCommand",
            "forms": [{
                "href": "https://wet-wade.herokuapp.com/{id}/open"
            }]
        }
    }
}